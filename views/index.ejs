<!DOCTYPE html>
<html lang="en">
<head>

    <title><%= title %></title>
</head>
<body>
<h1><%= title %></h1>
<p>welcome</p>
<%- include('components/bar',{name:name}) %>
<div class="list">
    <div class="row">
        <span>序号</span>
        <span>作者</span>
        <span>文章标题</span>
        <span>时间</span>
        <span>操作</span>
    </div>
    <% list.map(function(item,index){%>
    <div class="row">
                <span><%= index+1 %></span>
                <span><%= item.username %></span>
                <span><%= item.title %></span>
                <span><%= item.time %></span>
                <span>
                    <a href="">编辑</a>
                    <a href="">删除</a>
                </span>
                </div>
   <% }) %>

</div>
</body>
</html>